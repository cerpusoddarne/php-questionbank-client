{
  "name": "cerpus/questionbank-client",
  "description": "Client to talk do the Cerpus Questionset Service",
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.0",
    "kamermans/guzzle-oauth2-subscriber": "^1.0",
    "guzzlehttp/oauth-subscriber": "^0.6.0",
    "illuminate/support": ">=6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.0",
    "shrikeh/teapot": "^2.3",
    "fzaninotto/faker": "^1.5"
  },
  "authors": [
    {
      "name": "Thomas Horn Sivertsen",
      "email": "thomass@cerpus.com"
    },
    {
      "name": "Odd-Arne Johansen",
      "email": "odd-arne.johansen@cerpus.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cerpus\\QuestionBankClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cerpus\\QuestionBankClientTests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cerpus\\QuestionBankClient\\Providers\\QuestionBankClientServiceProvider"
      ]
    }
  }
}
